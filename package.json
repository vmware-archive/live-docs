{
  "name": "live-docs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:plainjs && npm run build:ng-live-docs && npm run build-live-docs-doc && npm run build-examples-doc && ng serve",
    "build": "ng build",
    "build:example-ng-app": "ng build example-ng-app",
    "build:example-ng-app-prod": "ng build example-ng-app --configuration production",
    "build:plainjs": "ng build plain-js-live-docs",
    "build:ng-live-docs": "ng build ng-live-docs && tsc -p projects/ng-live-docs/scripts/tsconfig.json",
    "postbuild:ng-live-docs": "npm run build:ng-live-docs:schematics",
    "build:ng-live-docs:schematics": "tsc -p projects/ng-live-docs/tsconfig.schematics.json",
    "postbuild:ng-live-docs:schematics": "cd projects/ng-live-docs && copyfiles \"schematics/**/collection.json\" \"schematics/**/schema.json\" \"schematics/**/files/**\" ../../dist/ng-live-docs",
    "test": "ng test",
    "test:ci:plainjs": "ng test plain-js-live-docs --watch=false --browsers ChromeHeadlessNoSandbox --code-coverage",
    "test:ci:ng-live-docs": "ng test ng-live-docs --watch=false --browsers ChromeHeadlessNoSandbox --code-coverage",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "build-live-docs-doc": "mkdirp projects/example-ng-app/gen && node dist/ng-live-docs/index.js projects/ng-live-docs/tsconfig.lib.json projects/ng-live-docs/src/public-api.ts projects/example-ng-app/gen/live-docs-doc.json",
    "build-examples-doc": "mkdirp projects/example-ng-app/gen && node dist/ng-live-docs/index.js projects/example-ng-app/tsconfig.app.json projects/example-ng-app/src/main.ts projects/example-ng-app/gen/example-ng-app-doc.json"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^12.2.16",
    "@angular/animations": "12.2.16",
    "@angular/common": "12.2.16",
    "@angular/compiler": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/forms": "12.2.16",
    "@angular/platform-browser": "12.2.16",
    "@angular/platform-browser-dynamic": "12.2.16",
    "@angular/router": "12.2.16",
    "@cds/core": "^5.0.1",
    "@clr/angular": "5.0.1",
    "@clr/icons": "5.0.1",
    "@clr/ui": "5.0.1",
    "@stackblitz/sdk": "1.3.0",
    "@types/jasmine-ajax": "^3.1.37",
    "@webcomponents/custom-elements": "^1.5.0",
    "@webcomponents/webcomponentsjs": "2.0.0",
    "angular-cli-ghpages": "0.6.2",
    "cross-spawn": "^7.0.3",
    "lit-html": "1.1.2",
    "prismjs": "1.23.0",
    "rbradford-compodoc": "1.1.11",
    "rxjs": "6.5.5",
    "schematics-utilities": "2.0.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@actions/core": "1.2.6",
    "@actions/github": "2.2.0",
    "@angular-devkit/architect": "^0.1202.16",
    "@angular-devkit/build-angular": "12.2.16",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "12.2.16",
    "@angular/compiler-cli": "12.2.16",
    "@angular/language-service": "12.2.16",
    "@types/jasmine": "^3.6.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.6.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "4.2.5",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mkdirp": "^1.0.4",
    "ng-packagr": "^12.2.7",
    "postcss": "^8.4.4",
    "prettier": "1.18.2",
    "pretty-quick": "2.0.0",
    "protractor": "~7.0.0",
    "ts-node": "^10.0.0",
    "tsickle": "0.39.1",
    "typescript": "4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged; npm run lint-fix"
    }
  }
}
